<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FYProjectNew" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/selfcolor_grey</item>
        <item name="colorPrimaryVariant">@color/SelfBlue</item>
        <item name="colorOnPrimary">@color/SelfBlue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/SelfBlue</item>
        <item name="colorSecondaryVariant">@color/SelfBlue</item>
        <item name="colorOnSecondary">@color/SelfBlue</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/SelfBlue</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="NumberPickerStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColorPrimary">@color/SelfBlue</item>
        <item name="android:colorControlNormal" tools:targetApi="lollipop">@color/SelfBlue</item>
    </style>


</resources>